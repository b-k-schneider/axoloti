<objdefs>
   <obj.normal id="lfsrburst 8" sha="15dc098dc7464595e42a982a7c86798389557f18">
      <sDescription>lfsr burst, 8bit = 255 samples, audio rate</sDescription>
      <author>Johannes Taelman</author>
      <license>BSD</license>
      <inlets>
         <bool32.rising name="trig" description="trigger"/>
      </inlets>
      <outlets>
         <frac32buffer.positive name="out" description="lfs burst"/>
      </outlets>
      <displays/>
      <params/>
      <attribs>
         <combo name="polynomial">
            <MenuEntries>
               <string>0x8E</string>
               <string>0x95</string>
               <string>0x96</string>
               <string>0xA6</string>
               <string>0xAF</string>
               <string>0xB1</string>
               <string>0xB2</string>
               <string>0xB4</string>
               <string>0xB8</string>
               <string>0xC3</string>
               <string>0xC6</string>
               <string>0xD4</string>
               <string>0xE1</string>
               <string>0xE7</string>
               <string>0xF3</string>
               <string>0xFA</string>
            </MenuEntries>
            <CEntries>
               <string>0x8E</string>
               <string>0x95</string>
               <string>0x96</string>
               <string>0xA6</string>
               <string>0xAF</string>
               <string>0xB1</string>
               <string>0xB2</string>
               <string>0xB4</string>
               <string>0xB8</string>
               <string>0xC3</string>
               <string>0xC6</string>
               <string>0xD4</string>
               <string>0xE1</string>
               <string>0xE7</string>
               <string>0xF3</string>
               <string>0xFA</string>
            </CEntries>
         </combo>
      </attribs>
      <code.declaration><![CDATA[uint32_t state;
int32_t count;
int32_t ntrig;
]]></code.declaration>
      <code.init><![CDATA[state = 0;
count = 0;
ntrig = 0;
]]></code.init>
      <code.krate><![CDATA[   if ((%trig%>0) && !ntrig) { state = 1; count=255; ntrig=1;}
   else { if (!(%trig%>0)) ntrig=0;}
]]></code.krate>
      <code.srate><![CDATA[if (count>0) {
  count--;
  if (state & 1)  {
     state = (state >> 1) ^ %polynomial%;
     %out% = 1<<27;
  } else {
    state = (state >> 1);
     %out% = 0;
  }
} else %out% = 0;
]]></code.srate>
   </obj.normal>
</objdefs>