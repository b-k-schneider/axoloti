      <license>BSD</license>
      <inlets/>
      <outlets>
         <int32 name="x" description="middle mouse button"/>
         <int32 name="y" description="middle mouse button"/>
         <bool32 name="left" description="left mouse button"/>
         <bool32 name="right" description="right mouse button"/>
         <bool32 name="middle" description="middle mouse button"/>
      </outlets>
      <displays/>
      <params/>
      <attribs/>
      <code.krate><![CDATA[%left% = hid_buttons[0];
%right% = hid_buttons[1];
%middle% = hid_buttons[2];
%x% = hid_mouse_x;
%y% = hid_mouse_y;
]]></code.krate>
   </obj.normal>
